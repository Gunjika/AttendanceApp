        android:textSize="24sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="199dp"
        tools:layout_editor_absoluteY="36dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="185dp"
